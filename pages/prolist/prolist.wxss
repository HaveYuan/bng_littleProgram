/* pages/prolist/prolist.wxss */
.container{
  padding-bottom: 98rpx;

}
.pro-wrap{
  display: flex;
  flex-wrap: wrap;
  padding: 58rpx 28rpx 0;
}
.pro-wrap>view{
  width: 200rpx;
  margin-bottom: 36rpx;
  margin-right: 47rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.pro-wrap>view:nth-child(3n+3){
  margin-right: 0;
}
.pro-img{
  width: 200rpx;
  height: 200rpx;
}
.pro-text{
  font-size: 24rpx;
  color: #424242;
}